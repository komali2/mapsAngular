{
    "env": {
        "browser": true,
        "es6": true,
        "jasmine": true,
        "node": true
    },
    "globals":{
        "angular": true,
        "google": true,
        "inject": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-unused-vars": [
            "error",
            {
                "varsIgnorePattern": "((^|, )(google|app|))+$"
            }
        ]
    }
}